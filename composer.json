{
    "name": "innmind/crawler",
    "type": "library",
    "description": "Library to extract meaningful informations out of a webpage",
    "keywords": ["crawler"],
    "homepage": "http://github.com/Innmind/Crawler",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Crawler/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/immutable": "~3.3",
        "innmind/http": "~4.0",
        "innmind/filesystem": "~4.0",
        "innmind/url-resolver": "~4.0",
        "innmind/time-continuum": "~2.0",
        "innmind/html": "~5.0",
        "innmind/colour": "~3.0",
        "innmind/math": "~5.0",
        "innmind/http-transport": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Crawler\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Crawler\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    }
}
